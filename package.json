  "scripts": {
    "test": "npm run jshint && npm run mocha",
    "jshint": "jshint src/. test/. --config",
    "start": "pm2 start src --no-daemon --node-args='--harmony-async-await'",
    "mocha": "mocha test/ --recursive",
    "inject": "gulp",
    "build": "npm run inject && webpack",
    "production": "npm run inject && rm -rf lib/ && babel -d lib/ src/ && cp -a src/email-templates lib/email-templates && webpack",
    "serve": "babel-watch src/",
    "watch": "npm run inject && webpack -w",
    "test-client": "karma start karma.conf.js"
  },
