{
  "name": "feathers-vue-two",
  "description": "A boiler plate template using Feathers 2 with Email Verification, Vue 2 with Server Side Rendering, stylus, scss, pug, babel, webpack, ES 6-8, login form, user authorization, and SEO",
  "version": "0.0.0",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Jon Paul Miles",
    "email": "codingfriend1@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server"
  },
  "engines": {
    "node": ">=7.6.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "start": "docker-compose up",
    "build-dev": "docker-compose build --no-cache",
    "qa": "docker-compose -f qa.yml up",
    "build-qa": "docker-compose -f qa.yml build --no-cache",
    "watch": "gulp && webpack -w",
    "serve": "nodemon --watch server --watch config server/",
    "test": "mocha test/ --recursive",
    "test-client": "cross-env NODE_ENV=test && mocha-webpack --webpack-config webpack.config.js --timeout 15000 --require client-tests/setup.js client-tests/**/*.spec.js --watch",
    "integration": "cross-env NODE_ENV=test && npm-run-all --parallel serve test-client",
    "dev": "npm-run-all --parallel watch serve",
    "build": "gulp && NODE_ENV=production webpack -p",
    "production": "cross-env NODE_ENV=production node server/"
  },
